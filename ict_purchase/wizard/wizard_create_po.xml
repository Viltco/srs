<odoo>
    <record id="view_create_purchase_wizard_form" model="ir.ui.view">
        <field name="name">Create Purchase Order</field>
        <field name="model">purchase.sale.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group col="4">
                    <!-- <field name="partner_id" required="1" options="{'no_create': True ,'no_quick_create': True, 'no_create_edit' : True}"/> -->


                </group>


                <field name="wizard_line">
                    <tree editable="bottom" create="0" delete="0">
                        <field name="product_id" readonly="1" force_save="1"/>
                        <field name="product_uom_qty"/>
                        <field name="price_unit"/>
                        <field name="partner_id" required="1"
                               options="{'no_create': True ,'no_quick_create': True, 'no_create_edit' : True}"/>
                        <field name="product_uom" invisible="1"/>
                        <field name="dicker_qty"  string="Dicker Qty"/>
                        <field name="dicker_data"  string="SP"/>
                        <field name="leader_qty"  string="Leader Qty"/>
                        <field name="leader_data"  string="SP"/>
                        <field name="ingram_qty"  string="Ingram Qty"/>
                        <field name="ingram_micro_data"  string="SP"/>
                        <field name="synnex_qty"  string="Synnex Qty"/>
                        <field name="synexx_data"  string="SP"/>
                        <field name="tax_id" widget="many2many_tags" invisible="1"/>
                    </tree>
                </field>


                <footer>
                    <button name="action_create_purchase_order" string="Create PO" type="object" class="btn-primary"/>
                    <button string="Cancel" type="object" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>

    </record>

</odoo>